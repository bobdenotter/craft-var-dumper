{
    "name": "markwff/craftvardumper",
    "description": "Adds Symfony Var-Dumper to Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-var-dumper"
    ],
    "support": {
        "docs": "https://github.com/markwff/craft-var-dumper/blob/master/README.md",
        "issues": "https://github.com/markwff/craft-var-dumper/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Wolff",
            "homepage": "https://github.com/markwff"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "hellonico/twig-dump-extension": "^1.0"
    },
    "autoload": {
        "psr-4": {
          "markwff\\craftvardumper\\": "src/"
        }
    },
    "extra": {
        "name": "Craft-Var-Dumper",
        "handle": "craft-var-dumper",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/markwff/craft-var-dumper/master/CHANGELOG.md",
        "class": "markwff\\craftvardumper\\CraftVarDumper"
    }
}
